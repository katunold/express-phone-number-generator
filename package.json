{
  "name": "express-phone-number-generator",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "DEBUG='random-number-generator:server' nodemon --exec babel-node server.js",
    "server:prod": "DEBUG='random-number-generator:server' node ./node_modules/.bin/babel-node server.js",
    "build": "babel ./src --out-dir dist --ignore ./node_modules",
    "lint": "node_modules/.bin/eslint ./src",
    "test": "NODE_ENV=test nyc mocha --require @babel/register --exit"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.17.3",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1",
    "sinon": "^7.3.2",
    "sinon-chai": "^3.3.0",
    "twig": "~0.10.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/node": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/register": "^7.5.5",
    "babel-eslint": "^10.0.2",
    "babel-plugin-istanbul": "^5.2.0",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-import": "^2.18.2",
    "mocha": "^6.2.0",
    "nodemon": "^1.19.1",
    "nyc": "^14.1.1"
  },
  "nodemonConfig": {
    "ignore": [
      "test/*",
      "docs/*"
    ],
    "ext": "twig,js,css"
  }
}
